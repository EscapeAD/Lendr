 <div class='container search-container'>

  <div class='row'>
    <div class='col-md-2 category-list'>





      <nav class="navbar navbar-default navbar-static-top" style="background-color:white" role="navigation">
          <div class="container">
              <div class="navbar-header">
                  <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                      <span class="sr-only">Toggle navigation</span>
                      <span class="icon-bar"></span>
                      <span class="icon-bar"></span>
                      <span class="icon-bar"></span>
                  </button>
              </div>

      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav">
        <li><%= link_to 'All Items', items_path %></li>
        <li><a href='' id='educationCat'>Education</a></li>
        <li><a href='' id='equipmentCat'>Equipment</a></li>
        <li><a href='' id='landscapeCat'>Landscape</a></li>
        <li><a href='' id='leisureCat'>Leisure</a></li>
        <li><a href='' id='outdoorCat'>Outdoor</a></li>
        <li><a href='' id='repairsCat'>Repairs</a></li>
        <!-- <li id="search-on-category"><input id="searchTextField" type="text" name="searchTxt"></li>
        <li><button id="searchBtn">Search</button></li> -->

        <li id="search-on-geo"><input id="searchGeoTextField" type="text" name="searchGeoTxt"></li>
        <li><button id="searchGeoBtn">Search Geo</button></li>
      </ul>
    </div>
  </div>
</nav>







    </div>

    <div class='col-md-10 search-results'>
      <div id="searchSec">
        <div id="searchList">
          <section id="allItems">
            <% @items.each do |item| %>

              <% if item.pictures.empty? %>
                <div class='col-md-4 item-box col-xs-6'>
                    <%= image_tag "http://epaper2.mid-day.com/images/no_image_thumb.gif"%><br/>
                    <%= link_to item.name, item_path(item) %>
                    <br>Owner: <%= item.user_id %>
                </div>

              <% else %>

                <% if item.pictures != nil %>
                  <div class='col-md-4 item-box col-xs-6'>
                    <div class="item_image">
                      <%= link_to image_tag(item.pictures.first.image.url(:medium), class: 'main-image'),item_path(item) %>
                    </div>

                    <div class="item-info">
                      <%= link_to item.name, item_path(item) %>
                      <br>Owner: <%= User.find(item.user_id).first_name %> <%= User.find(item.user_id).last_name %>
                    </div>
                  </div>
                <% end %>
              <% end %>
            <% end %>
          </section>
        </div>
      </div>
    </div>
  </div>

</div>
