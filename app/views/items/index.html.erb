<h1>All Items</h1>

<ul>
<% @items.each do |item| %>
  <%= link_to item.name, item_path(item) %>
  <li>Description: <%= item.description %></li>
  <li>Type: <%= item.item_type %></li>
  <li>Owner: <%= item.user_id %></li>
  <li class = "item_image">Image: <br><%= image_tag item.image.url %></li>
  <br>
<% end %>
</ul>
