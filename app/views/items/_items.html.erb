<h2>Search Results</h2>


<% if searchItemList.empty? %>
<em>  No results found </em>
<% else %>
<% searchItemList.each do |item| %>
      <div class = "item_image"><%= image_tag item.pictures.first.image.url(:thumb) %></div>
      <div class = "item-info">
        <%= link_to item.name, item_path(item) %>
        <br>Owner: <%= item.user_id %>
      </div>
      <br>
    <% end %>
<% end %>
