<body data-turbolinks="false">
<div id="fullpage">

	<div class="section active" data-anchor="page1" id="section1">

		<div class="slide" data-anchor="slide1" id="slider3">
			<div class="slide3-text cover-slider">
        <h1 class ="slider-header">Looking for something to borrow?</h1>
        <h2 class = "slider-description">We have what you need.</h2>
        <!-- <a class = "slider-box-link">(search bar here)</a> -->
        <div class="search-text">
          <input id="mainSearchTextField" class="searchTextField" type="text" name="mainSearchTxt">
          <button id="mainSearchBtn" class="btn btn-default"><%= link_to "Search", items_path %></button>
        </div>
			</div>
		</div>

    <div class="slide" data-anchor="slide1" id="slider1">
			<div class="slide1-text cover-slider">
        <h1 class ="slider-header">Make connections.</h1>
        <h2 class = "slider-description">Meet lendr users with common interests.</h2>
        <a class = "slider-box-link">join now</a>
			</div>
		</div>

    <div class="slide" data-anchor="slide2" id="slider2">
			<div class="slide2-text cover-slider">
        <h1 class ="slider-header">significant objects</h1>
        <h2 class = "slider-description">items with stories of<br>where they have been</h2>
        <a class = "slider-box-link">be part of something significant</a>
			</div>
		</div>
  </div>

	<div class="section" data-anchor="page2" id="section2">
		<%= image_tag "icon1.svg", class:"timeline-icon img-responsive", id: "icon1"%>
		<%= image_tag "icon2.svg", class:"timeline-icon img-responsive", id: "icon2"%>
		<%= image_tag "icon3.svg", class:"timeline-icon img-responsive", id: "icon3"%>
		<%= image_tag "tent.svg", id: "tent-icon"%>
		<%= image_tag "light-bulb.svg", id: "light-bulb-icon"%>
		<%= image_tag "wrench.svg", id: "wrench-icon"%>
		<div class = "container">
			<div class="timeline main-feature">
				<div class = "timeline-map col-md-6">
				</div>
	      <div class = "timeline-text col-md-6">
	  			<h1>Timeline of Item Background</h1>
	        <p>
	          Every item has its own history. We want to hear all about it.
	          Add your objects to our library to create your story or borrow items to continue its adventure.
	        </p>
	      </div>
			</div>
		</div>
	</div>

	<div class="container section" data-anchor="page3" id="section3">
		<div class="location main-feature">
      <%= image_tag "dotted_lines.svg", id:"user-lines" %>
      <%= image_tag "city.svg", id:"city-landscape" %>
      <%#= image_tag "user-profile-city.svg", id:"user-icons" %>

			<div class = "user1_card effect_random">
